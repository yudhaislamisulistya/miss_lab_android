<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.leadevs.misslab">


    <uses-permission android:name="android.permission.INTERNET"/>
    
    <application
        android:allowBackup="true"
        android:icon="@drawable/iclabs"
        android:label="@string/app_name"
        android:roundIcon="@drawable/iclabs"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".ui.praktikan.PraktikanActiveLabAsistenActivity"
            android:theme="@style/Main">
        </activity>
        <activity
            android:name=".NotificationActivity"
            android:theme="@style/Main">
        </activity>
        <activity
            android:name=".ui.praktikan.HomePraktikanActivity"
            android:theme="@style/HomePraktikanActivity" />
        <activity
            android:name=".DetailPraktikum"
            android:theme="@style/Main" />
        <activity
            android:name=".DetailPengajarDosen"
            android:theme="@style/Main" />
        <activity
            android:name=".DetailPengajarAsisten"
            android:theme="@style/Main" />
        <activity
            android:name=".DetailInformasi"
            android:theme="@style/Main" />
        <activity
            android:name=".Login"
            android:theme="@style/Login" />
        <activity
            android:name=".MainActivity"
            android:theme="@style/Main" />
        <activity
            android:name=".SplashScreen"
            android:theme="@style/SplashTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <service
            android:name=".Notification">
            <intent-filter>
                <action
                    android:name="com.google.firebase.MESSAGING_EVENT"/>
            </intent-filter>
        </service>

    </application>

</manifest>